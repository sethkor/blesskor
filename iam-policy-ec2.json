{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "kms:CreateGrant",
                "kms:GenerateRandom",
                "kms:Encrypt",
                "kms:ListAliases"
            ],
            "Resource": "arn:aws:kms:ap-southeast-2:293499315857:alias/:sethkor",
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject",
                "iam:PassRole",
                "ec2:AssociateAddress",
                "lambda:ListFunctions"
            ],
            "Resource": "*",
            "Effect": "Allow"
        },
        {
            "Action": [
                "iam:PassRole"
            ],
            "Resource": "arn:aws:iam::293499315857:role/BastionIamLambdaRole",
            "Effect": "Allow"
        },
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:PutItem",
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:DeleteItem"
            ],
            "Resource": "arn:aws:dynamodb:ap-southeast-2:293499315857:table/credential-store"
        },
        {
            "Action": [
                "lambda:CreateFunction",
                "lambda:DeleteFunction"
            ],
            "Resource": "arn:aws:lambda:ap-southeast-2:293499315857:function:blesskor-bastion",
            "Effect": "Allow"
        }
        
    ]
}